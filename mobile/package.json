    "scripts": {
        "start": "expo start --dev-client",
        "start-with-target": "expo run:android --port 8088 --device",
        "prebuild": "expo prebuild --clean --platform android",
        "build-apk": "node build.js",
        "build-aab": "node build.js --bundle",
        "reset-project": "node ./scripts/reset-project.js",
        "refresh-common": "npm uninstall @cbr/common && npm pack ../common && npm install cbr-common-1.0.0.tgz",
        "test": "jest --watchAll",
        "lint": "expo lint",
        "android": "expo run:android",
        "ios": "expo run:ios"
    },
    "jest": {
        "preset": "jest-expo"
    },
